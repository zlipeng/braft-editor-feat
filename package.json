    "build": "cross-env NODE_ENV=production webpack --config ./playground/webpack/webpack.config.prod --progress --colors && node ./playground/webpack/scss-output.js",
    "build-output-css": "node ./playground/webpack/scss-output.js",
    "test": "npm run lint"
